{
    "name": "antidot-fw/container",
    "type": "library",
    "description": "PSR-11 container implementation made for Antidot Framework",
    "keywords": [
        "antidot-fw",
        "container"
    ],
    "homepage": "https://github.com/antidot-fw/container",
    "license": "MIT",
    "authors": [
        {
            "name": "Koldo Picaza",
            "homepage": "https://github.com/kpicaza",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpro/grumphp": "^0.17.1 || ^0.19.0",
        "phpstan/phpstan": "^0.11.15",
        "phpunit/phpunit": ">=7.5",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^4.3 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Antidot\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntidotTest\\Container\\": "tests"
        }
    },
    "scripts": {
        "check-all": [
            "@check-style",
            "@inspect",
            "@test"
        ],
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "inspect": "phpstan analyse src -l7 --ansi"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
