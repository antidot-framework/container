{
    "name": "antidot-fw/container",
    "type": "library",
    "description": "PSR-11 container implementation made for Antidot Framework",
    "keywords": [
        "antidot-fw",
        "container"
    ],
    "homepage": "https://github.com/antidot-fw/container",
    "license": "MIT",
    "authors": [
        {
            "name": "Koldo Picaza",
            "homepage": "https://github.com/kpicaza",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpro/grumphp": "^1.0.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Antidot\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntidotTest\\Container\\": "tests"
        }
    },
    "scripts": {
        "check-all": [
            "@cs-check",
            "@inspect",
            "@test"
        ],
        "test": "phpunit",
        "cs-check": "phpcs src tests",
        "cs-fix": "phpcbf src tests",
        "inspect": "phpstan analyse src -l7 --ansi"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
